{
    "name": "bennent/laravel-plugins-admin",
    "description": "Laravel plugins package base on auth backend with LayUI",
    "keywords": [
        "laravel",
        "auth",
        "plugin",
        "rbac",
        "permissions",
        "admin",
        "php",
        "roles"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "bennent_G",
            "email": "guolixun@163.com",
            "homepage": "https://blog,csdn.net/glx490676405",
            "role": "Developer"
        }
    ],
    "require": {
        "mews/captcha": "^3.2",
        "mindplay/annotations": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Bennent\\Geauth\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bennent\\Geauth\\GeauthServiceProvider"
            ],
            "aliases": {
                "geauth": "Bennent\\Geauth\\Facades\\Geauth"
            }
        }
    }
}
